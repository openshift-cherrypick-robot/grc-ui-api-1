{
  "name": "@stolostron/grc-ui-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./output/index.js",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./output/index.js",
    "start:development": "cross-env NODE_ENV=development node ./output/index.js",
    "build-server": "babel -d ./output ./src -s",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build",
    "clean": "rm -rf output && mkdir output",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js --fix .",
    "watch": "nodemon --inspect ./src/index.js --exec babel-node",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stolostron/grc-ui-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.4.1"
  },
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-async-to-generator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@stolostron/security-middleware": "^1.0.5",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.25.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "log4js": "^6.4.5",
    "lru-cache": "^6.0.0",
    "morgan-body": "^2.6.6",
    "nconf": "^0.12.0",
    "nocache": "^3.0.1",
    "requestretry": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.0.6",
    "jest-sonar-reporter": "^2.0.0",
    "jest-tap-reporter": "^1.9.0",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.6"
  }
}
